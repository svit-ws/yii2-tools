{
    "name": "svit-ws/yii2-tools",
    "description": "Common tools, mixins, helpers, behaviors, validators & etc.",
    "type": "yii2-extension",
    "license": "MIT",
    "keywords": [
        "yii2",
        "extension",
        "tools"
    ],
    "authors": [
        {
            "name": "Seva Vick",
            "email": "info@svit.ws"
        }
    ],
    "require": {
        "php": ">=5.6",
        "yiisoft/yii2": "^2.0.20"
    },
    "autoload": {
        "psr-4": {
            "svit\\tools\\": "src",
            "svit\\tools\\tests\\": "tests"
        }
    },
    "require-dev": {
        "yiisoft/yii2-queue": "~2.0.2",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-db": "^1.0.1"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
